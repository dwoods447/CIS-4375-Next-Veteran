<h1 class="page-title">Frequently Asked Questions</h1>

  <style>
    .question, .answer{
      font-size: 1.1em;
      padding: 1em;
    }

  </style>

  <ul>
    <% @frequently_asked_questions.each do |frequently_asked_question| %>
        <li class="question"><span style="color: #fff; font-weight: 900;">Question:</span>&nbsp;<%= frequently_asked_question.question %></li>
        <li class="answer"><span style="color: #fff; font-weight: 900;">Answer:</span>&nbsp;<%= frequently_asked_question.answer %></li>
    <% end %>
  </ul>

<div class="row">
  <div class="col-lg-12 col-md-12 col-sm-12">
    <%= link_to student_kiosk_index_path do %>
        <span class="back-link-style"><strong>Back to Student Kiosk</strong>&nbsp;<i class="fa fa-arrow-left" aria-hidden="true"></i></span>
    <%end %>
  </div>
</div>


<script>
    var body = document.getElementsByTagName("body")[0];
    body.style.backgroundColor = "#003768";
    body.style.color = "#fff";
</script>

